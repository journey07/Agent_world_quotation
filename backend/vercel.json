{
  "version": 2,
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "regions": ["icn1"],
  "headers": [
    {
      "source": "/api/quote/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://agent-world-quotation.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS, PUT, DELETE"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-Requested-With"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/calculate",
      "destination": "/api/quote/calculate"
    },
    {
      "source": "/inquiries",
      "destination": "/api/quote/inquiries"
    },
    {
      "source": "/preview-image",
      "destination": "/api/quote/preview-image"
    },
    {
      "source": "/excel",
      "destination": "/api/quote/excel"
    },
    {
      "source": "/generate-3d-installation",
      "destination": "/api/quote/generate-3d-installation"
    },
    {
      "source": "/health",
      "destination": "/api/quote/health"
    },
    {
      "source": "/dashboard-stats",
      "destination": "/api/quote/dashboard-stats"
    },
    {
      "source": "/agent-toggle",
      "destination": "/api/quote/agent-toggle"
    },
    {
      "source": "/agent-status",
      "destination": "/api/quote/agent-status"
    },
    {
      "source": "/verify-api",
      "destination": "/api/quote/verify-api"
    },
    {
      "source": "/pdf",
      "destination": "/api/quote/pdf"
    }
  ]
}
